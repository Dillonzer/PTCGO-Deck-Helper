<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PTCGO_Deck_Helper"
        xmlns:Custom_Controls="clr-namespace:PTCGO_Deck_Helper.Custom_Controls" x:Class="PTCGO_Deck_Helper.SetPrizes"
        mc:Ignorable="d"
        Title="Set Prizes" Height="800" Width="1000"
    MinHeight="800" MinWidth="1000">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" x:Name="stp_Pokemon" CanVerticallyScroll="True"/>
        <StackPanel Grid.Column="1" x:Name="stp_Trainers" CanVerticallyScroll="True"/>
        <StackPanel Grid.Column="2" x:Name="stp_Energy" CanVerticallyScroll="True"/>
    </Grid>
</Window>
